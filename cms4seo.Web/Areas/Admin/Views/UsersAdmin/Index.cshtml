@model IEnumerable<cms4seo.Data.IdentityModels.ApplicationUser>

@{
    ViewBag.Title = AdminResources.CommonIndex;
}

<h2>@AdminResources.CommonIndex</h2>

<p>
    @Html.ActionLink(AdminResources.CommonCreate, "Create",
        null, new {@class = "btn btn-success"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink(AdminResources.CommonEdit, "Edit", new {id = item.Id}) |
                @Html.ActionLink(AdminResources.CommonDetails, "Details", new {id = item.Id}) |
                @Html.ActionLink(AdminResources.CommonDelete, "Delete", new {id = item.Id})
            </td>
        </tr>
    }

</table>